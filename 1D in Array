#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int student[4000],i,size;
    scanf("%d\n",&size);
    for(i=0;i<size;i=i+1)
      scanf("%d",&student[i]);
    int sum=0;
    for(i=0;i<size;i=i+1)
      sum=sum+student[i];
    printf("%d",sum);
    return 0;
}
